<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#FF000000</color>
    <color name="light_blue_200">#FF81D4FA</color>
    <color name="light_blue_50">#FFE1F5FE</color>
    <color name="light_blue_600">#FF039BE5</color>
    <color name="light_blue_900">#FF01579B</color>
    <color name="purple_500">#6200EE</color>
    <color name="white">#FFFFFFFF</color>
    <dimen name="widget_margin">0dp</dimen>
    <string name="accessibility_service_description">사용자 앱 클릭 횟수 추격 서?비?스?</string>
    <string name="add_widget">Add widget</string>
    <string name="appString">앱 이미지</string>
    <string name="app_icon_desc">앱 아이콘입니다.</string>
    <string name="app_name">AI_Customizing</string>
    <string name="app_widget_description">This is an app widget description</string>
    <string name="appwidget_text">기본 앱</string>
    <style name="Base.Theme.AI_Customizing" parent="Theme.Material3.DayNight.NoActionBar">
        
        
    </style>
    <style name="Theme.AI_Customizing" parent="Base.Theme.AI_Customizing"/>
    <style name="Theme.AI_Customizing.AppWidgetContainer" parent="Theme.AI_Customizing.AppWidgetContainerParent">
        
        <item name="appWidgetPadding">16dp</item>
    </style>
    <style name="Theme.AI_Customizing.AppWidgetContainerParent" parent="@android:style/Theme.DeviceDefault">
        
        <item name="appWidgetRadius">16dp</item>
        
        <item name="appWidgetInnerRadius">8dp</item>
    </style>
    <style name="Widget.AI_Customizing.AppWidget.Container" parent="android:Widget">
        <item name="android:id">@android:id/background</item>
        <item name="android:background">?android:attr/colorBackground</item>
    </style>
    <style name="Widget.AI_Customizing.AppWidget.InnerView" parent="android:Widget">
        <item name="android:background">?android:attr/colorBackground</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
    </style>
    <declare-styleable name="AppWidgetAttrs">
        <attr format="dimension" name="appWidgetPadding"/>
        <attr format="dimension" name="appWidgetInnerRadius"/>
        <attr format="dimension" name="appWidgetRadius"/>
    </declare-styleable>
</resources>